{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
      {
      "src": "*pannel/public/.png",
      "use": "@vercel/static"
    },
      },
      {
      "src": "*pannel/public/.ico",
      "use": "@vercel/static"
    },
      {
      "src": "*edit/.ico",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
